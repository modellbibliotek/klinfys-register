					description = <"Unterhalb der Körpermitte, oft in Richtung der Füße.">
				>
				["at0071"] = <
					text = <"Anterior">
					description = <"In Richtung der Vorderseite oder der ventralen Oberfläche des Körpers.">
				>
				["at0072"] = <
