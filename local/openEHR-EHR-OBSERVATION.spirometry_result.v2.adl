				["at0064"] = <
					text = <"*SVC (en)">
					description = <"*Slow vital capacity (SVC) is the maximal volume of air that can be slowly exhaled from a position of full inspiration. Also known as Expiratory vital capacity (EVC). Equal to IRV + TV + ERV. (en)">
				>
				["at0065"] = <
					text = <"*FVC (en)">
					description = <"*Forced vital capacity (FVC) is the maximal volume of air that can be forcibly exhaled from a position of full inspiration. (en)">
