                </term_definitions>
                <term_definitions code="suture_site_vascular_access::4">
                    <items id="text">Insticksstlle fr tunnelerad kateter</items>
                </term_definitions>
                <term_definitions code="suture_site_vascular_access::5">
                    <items id="text">Vingar</items>
                </term_definitions>
