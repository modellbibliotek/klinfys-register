        </term_definitions>
        <term_definitions code="medical_device_introducer::3">
            <items id="text">Temporr Pacemaker - introducer</items>
        </term_definitions>
        <term_definitions code="medical_device_introducer::4">
            <items id="text">Aortaballongpump</items>
        </term_definitions>
